#lang racket
(require simple-matrix)

(define verbose #t)

; returns the index of the greatest element in a list
; use min #t for the least element
(define (min-index l)
  (define greatest (apply min l))
  (define (find sublist target [index 0])
    (when verbose (printf "? ~a in col ~a\n" target index))
    (if (= target (car sublist))
        index
        (find (cdr sublist) target (+ 1 index))))
  (find l greatest))

(define (max-index l)
  (define greatest (apply max l))
  (define (find sublist target [index 0])
    (when verbose (printf "? ~a in ~a\n" target index))
    (if (= target (car sublist))
        index
        (find (cdr sublist) target (+ 1 index))))
  (find l greatest))

; get the value of row r, column c in matrix M
(define (m-ref M r c) (list-ref (list-ref M r) c))

; multiplies a row by s in matrix M
(define (r* M s row)
  (transpose (matrix* (transpose M)
    (build-list (length M) (lambda(i)
      (build-list (length M) (lambda(j)
        (if (= i j) (if (= row i) s 1) 0))))))))

; does a row operation to matrix M
; adds row r-from, scaled by s, to row r-to
(define (rop M s r-from r-to)
  ;(when verbose (printf "~a\n\n~aR~a + R~a\n\n" M s r-from r-to))
  (matrix+ M
    (build-list (length M) (lambda(r)
      (build-list (length (car M)) (lambda(c)
        (if (= r-to r) (* s (list-ref (list-ref M r-from) c)) 0)))))))

; normalize the pivot row at row r and column c, then reduce other rows
(define (do-pivot M pivot)
  (when verbose (printf "pivoting at ~a\n\n" pivot))
  (define (reduce mat [row 0])
    (if (>= row (length mat))
        mat
        (if (= row (car pivot))
            (reduce mat (+ row 1))
            (reduce (rop mat (- (m-ref mat row (cdr pivot))) (car pivot) row) (+ row 1)))))
  (reduce (r* M (/ (list-ref (list-ref M (car pivot)) (cdr pivot))) (car pivot))))

; choose the pivot column
(define (pivot-col M [max #t])
  (let ([obj-row (car (reverse M))])
    (if max (min-index obj-row) (max-index obj-row))))

; returns the pivot location for matrix M as a pair '(row . column)
(define (best-pivot M [max #t])
  (let* ([col (pivot-col M max)]
         [pivotcol (list-ref (transpose M) col)]
         [RHS (car (reverse (transpose M)))]
         [ratios (build-list (length pivotcol) (lambda(row) (let ([piv (list-ref pivotcol row)]) (if(<= piv 0) +inf.0 (/ (list-ref RHS row) piv)))))]
         [row (if max (min-index ratios) (max-index ratios))])
    (when verbose (printf "pivotcol: ~a\nRHS: ~a\n\n" pivotcol RHS))
    (when verbose (printf "ratios: ~a\n\n" ratios))
    (cons row col)))

; returns true if last row is negative except the last element
; set maximize to false for minimization (checks for positive last row)
(define (optimal? M [max #t])
  (define is-optimal #t)
  (for ([v (cdr (reverse (car (reverse M))))]) ; last row without solution element
    (if max
        (when (< v 0) (set! is-optimal #f))
        (when (> v 0) (set! is-optimal #f))))
  is-optimal)

(define (invert-pivots M col)
  (define (next-row nextM row)
    (if (>= row (- (length M) 1))
        nextM
        (if (and (> 0 (m-ref M row col)) (= 0 (m-ref nextM row (- (length (car M)) 1))))
            (next-row (r* nextM -1 row) (+ 1 row))
            (next-row nextM (+ 1 row)))))
  (next-row M 0))

; check if optimal, choose a pivot, reduce, repeat
(define (simplex tableau [maximum #t])
  (when verbose (printf "obj: ~a\n\n" (car (reverse tableau))))
  (if (optimal? tableau maximum)
      tableau
      (if (> 0 (apply max (list-ref (transpose tableau) (pivot-col tableau))))
          (let ([inv-piv-tab (invert-pivots tableau (pivot-col tableau))])
            (simplex (do-pivot inv-piv-tab (best-pivot inv-piv-tab maximum)) maximum))
          (simplex (do-pivot tableau (best-pivot tableau maximum)) maximum))))

